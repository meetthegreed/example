mixin head-line()
  +b.DIV.head-line
    +e.DIV.container
      +b.DIV.head-line-item
        +e.I.icon(class="mdi mdi-map-marker")
        |г. Киев, ул. Михайловская, 20
      +b.DIV.head-line-item
        +e.I.icon(class="mdi mdi-av-timer")
        |ПН - ВС, с 09:00 до 21:00
      +b.DIV.head-line-item
        +e.I.icon(class="mdi mdi-phone")
        |+38(044)275-75-75
      +b.DIV.head-line-item 
        +e.I.icon(class="mdi mdi-email")
        |hello@printing.com
